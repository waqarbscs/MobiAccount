<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.OneWindow.sol.MobiAccount">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.OneWindow.sol.MobiAccount.MainActivity"
            android:theme="@style/Splash"/>
        <activity android:name="com.OneWindow.sol.MobiAccount.Splash"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.OneWindow.sol.MobiAccount.Register"
            android:label="Register"
            android:parentActivityName="com.OneWindow.sol.MobiAccount.Login"/>
        <activity android:name="com.OneWindow.sol.MobiAccount.Login"
            android:label="Login"/>
        <activity android:name="com.OneWindow.sol.MobiAccount.About"
            />
        <activity
            android:name="com.OneWindow.sol.MobiAccount.ItemDetails"
             />
        <activity android:name="com.OneWindow.sol.MobiAccount.Status"
            android:label="Account Heads"/>
        <activity android:name=".View1"
            android:label="View"/>
        <activity android:name="com.OneWindow.sol.MobiAccount.ShowBudget"
            android:label="View"/>
        <activity android:name="com.OneWindow.sol.MobiAccount.Account"
            android:label="Status"></activity>
    </application>

</manifest>